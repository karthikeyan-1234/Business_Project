{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "InventoryDBConnection": "Server=(localdb)\\InventoryServer;Database=InventoryDB;Trusted_Connection=true;"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "Exchange": "ERP"
  },
  "PurchaseQueue": {
