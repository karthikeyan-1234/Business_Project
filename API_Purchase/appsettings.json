{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PurchaseDBConnection": "Server=(localdb)\\PurchaseServer;Database=PurchaseDB;Trusted_Connection=true;"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "Exchange": "ERP"
  },
  "PurchaseQueue": {
    "Queue": "Purchase",
    "Request_Key": "PurchaseRequest",
    "Response_Key": "PurchaseResponse",
    "Update_Key": "PurchaseUpdate"
  },
  "InventoryQueue": {
    "Queue": "Inventory",
    "Request_Key": "InventoryRequest",
    "Response_Key": "InventoryResponse",
    "Update_Key": "InventoryUpdate"
  }
}
