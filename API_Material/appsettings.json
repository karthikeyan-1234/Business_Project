{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MaterialDBConnection": "Server=(localdb)\\MaterialServer;Database=MaterialDB;Trusted_Connection=true;"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "Exchange": "ERP"
  },
  "PurchaseQueue": {
    "Queue": "Purchase",
    "Request_Key": "PurchaseRequest",
    "Response_Key": "PurchaseResponse",
    "Update_Key": "PurchaseUpdate"

  },
  "InventoryQueue": {
    "Queue": "Inventory",
    "Request_Key": "InventoryRequest",
    "Response_Key": "InventoryResponse",
    "Update_Key": "InventoryUpdate"
  },
  "MaterialQueue": {
    "Queue": "Material",
    "Request_Key": "MaterialRequest",
    "Response_Key": "MaterialResponse",
    "Update_Key": "MaterialUpdate"
  }
}
